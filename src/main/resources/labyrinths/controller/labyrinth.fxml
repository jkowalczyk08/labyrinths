<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="labyrinths.controller.LabyrinthController">
   <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <HBox prefHeight="100.0" prefWidth="194.0">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets top="20.0" />
         </padding>
         <VBox.margin>
            <Insets top="50.0" />
         </VBox.margin>
         <Pane HBox.hgrow="ALWAYS">
            <opaqueInsets>
               <Insets />
            </opaqueInsets>
            <padding>
               <Insets right="20.0" />
            </padding>
         </Pane>
         <Button fx:id="dfsBtn" mnemonicParsing="false" text="Perfom DFS">
            <HBox.margin>
               <Insets right="20.0" />
            </HBox.margin>
            <font>
               <Font size="25.0" />
            </font>
         </Button>
         <Button fx:id="clearBtn" mnemonicParsing="false" text="Clear">
            <HBox.margin>
               <Insets />
            </HBox.margin>
            <font>
               <Font size="25.0" />
            </font>
         </Button>
         <Pane HBox.hgrow="ALWAYS" />
      </HBox>
      <Pane fx:id="labyrinthPane" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="100.0" left="100.0" right="100.0" />
         </VBox.margin>
      </Pane>
   </VBox>
</AnchorPane>
